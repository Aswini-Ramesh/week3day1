public class BankInfo {
public void saving() {
	System.out.println("Saving Account Balance:25,000");
}
public void fixed() {
	System.out.println("My Fixed Balance=50,000");
}
public void deposit() {
	System.out.println("Now My Deposit Amount:10,000");
}
}
public class AxisBank extends BankInfo{
public void deposit() {
	System.out.println("My Money 5,000");
}
	public static void main(String[] args) {
		AxisBank ab= new AxisBank();
		ab.saving();
		ab.fixed();
		ab.deposit();
	}

}
